<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

        <variable
            name="personUserInfoBean"
            type="com.millet.mylibrary.bean.result.PersonUserInfoBean" />
    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@{personUserInfoBean.nickName}"
            android:textSize="40pt" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@{personUserInfoBean.birth}"
            android:textSize="40pt" />

        <TextView
            android:id="@+id/tv_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@{personUserInfoBean.sexType}"
            android:textSize="40pt" />

    </LinearLayout>

</layout>